@inherits UmbracoViewPage<Home>
@{
    Layout = null;
}

<!doctype html>
<html lang="en" class="h-100">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Reading Room | Umbraco Cloudflare Stream</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <style>
        :root {
            --bs-primary-rgb: 253, 210, 52;
            --bs-secondary-rgb: 88, 75, 155;
            --bs-info-rgb: 138, 228, 230;
            --bs-body-bg-rgb: 246, 246, 246;
            --bs-font-sans-serif: proxima-nova, Arial, Helvetica, sans-serif;
        }
    </style>
</head>
<body>

<div class="py-5 bg-primary text-center">
    <div class="my-5"></div>
    <h1 class="display-1">Reading Room</h1>
    <h2 class="display-6 fw-lighter">Umbraco Cloudflare Stream</h2>
    <div class="my-5"></div>
</div>

@if (Model.BetterPicker != null)
{
    var value = Model.BetterPicker;
    var duration = $"{value.Duration}s";
    var size = $"{value.Size / 1024 / 1024:0.00}MB";
    var editUrl = "/umbraco#/media/media/edit/" + value.Id;
    var poster = string.Empty;
    if (Model.Poster != null)
    {
        poster = Model.Poster.Url(mode: UrlMode.Absolute);
    }

    <div class="bg-secondary py-5">
        <div class="container">
            <div class="ratio ratio-16x9">
                <cf-stream video="@value" poster-url="@poster" class="m-auto"/>
            </div>
            <div class="pt-5 text-white">
                <span class="me-2">
                    <i class="bi bi-clock me-1"></i>@duration
                </span>
                <span class="me-2">
                    <i class="bi bi-file-binary me-1"></i>@size
                </span>
                <a class="text-decoration-none me-2 text-white" target="_blank" href="@value.Playback.Url">
                    <i class="bi-link-45deg me-1"></i>Video Link</a>
                <a class="text-decoration-none me-2 text-white" target="_blank" href="@value.Playback.DashManifestUrl">
                    <i class="bi-link-45deg me-1"></i>Dash Manifest Link</a>
                <a class="text-decoration-none me-2 text-white" target="_blank" href="@value.Playback.HlsManifestUrl">
                    <i class="bi-link-45deg me-1"></i>Hls Manifest Link</a>
                <a class="text-decoration-none me-2 text-white" target="_blank" href="@editUrl">
                    <i class="bi-wrench me-1"></i>Umbraco Link</a>
            </div>
        </div>
    </div>
}
